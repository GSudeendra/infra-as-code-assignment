# AWS Configuration
# Copy this file to .env and modify as needed
# DO NOT commit .env file to version control

# AWS Profile (recommended approach)
# ⚠️  SECURITY: Use AWS SSO profile instead of hardcoded credentials
AWS_PROFILE=twbeach

# Alternative: Direct AWS credentials (not recommended for SSO)
# AWS_ACCESS_KEY_ID=your_access_key
# AWS_SECRET_ACCESS_KEY=your_secret_key
# AWS_SESSION_TOKEN=your_session_token

# AWS Region
AWS_REGION=ap-southeast-2

# Terraform Configuration
TF_VAR_environment=dev
TF_VAR_aws_region=ap-southeast-2

# API Gateway URL (will be set automatically after deployment)
API_GATEWAY_URL=https://your-api-gateway-url.execute-api.ap-southeast-2.amazonaws.com 